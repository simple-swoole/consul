{
    "name": "simple-swoole/consul",
    "description": "A Consul Client.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "consul",
        "consul-client"
    ],
    "require": {
        "php": ">=7.2",
        "hyperf/guzzle": "~2.1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Simps\\Consul\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix"
    }
}
